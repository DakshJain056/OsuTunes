{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "sql:default",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select",
    "core:default",
    "store:default",
    "store:allow-delete",
    "store:allow-get",
    "store:allow-set",
    "store:allow-has",
    "store:allow-save",
    "store:allow-load",
    "core:event:default",
    "deep-link:default",
    "deep-link:allow-get-current",
    "dialog:default"
  ]
}